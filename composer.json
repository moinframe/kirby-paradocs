{
    "name": "moinframe/kirby-paradocs",
    "description": "Auto-generate beautiful documentation pages using markdown",
    "license": "MIT",
    "type": "kirby-plugin",
    "version": "0.0.0",
    "authors": [
        {
            "name": "Justus Kraft",
            "email": "justus@moinfra.me"
        }
    ],
    "require": {
        "getkirby/composer-installer": "^1.1",
        "getkirby/cms": "^4.6"
    },
    "suggest": {
        "phiki/phiki": "^1.1 - Enhanced syntax highlighting"
    },
    "autoload": {
        "psr-4": {
            "Moinframe\\Paradocs\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    }
}
